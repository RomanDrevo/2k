{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "GET/business response schema",
  "type" : "object",
  "properties" : {
    "user_role":{"type":"string","description":"one of influencer/guest/liker/owner/advertiser"},
    "id" : {"type" : "integer"},
    "created_at":{"type": "string", "description": "valid timestamp format 2012-01-19 17:21:00 CST"},
    "name": {"type": "string"},
    "headline":{"type": "string"},
    "description":{"type": "string"},
    "tags":{"type":"array","items":{"type":"string"}},
    "website":{"type": "string"},
    "registered_domain":{"type": "string"},
    "contact_email":{"type": "string"},
    "contact_name":{"type": "string"},
    "contact_phone_number":{"type": "string"},
    "contact_user":{"type": "string", "description":"email of contact person"},
    "default_advertiser_language":{"type": "string", "description":"currently either HEBREW or ENGLISH"},
    "default_campaign_language":{"type": "string", "description":"currently either HEBREW or ENGLISH"},
    "twokey_am":{"type": "string"},
    "twokey_csm":{"type": "string"},
    "is_deleted":{"type": "boolean"},
    "deleted_by":{"type": "string","description":"the email of the user who chose to delete the business"},
    "deleted_at":{"type": "string", "description": "valid timestamp format 2012-01-19 17:21:00 CST"},
    "profile_media_url":{"type":"string"},
    "profile_media_id":{"type":"integer"},
    "profile_media_type":{"type":"string","description":"either GIF/IMAGE/VIDEO"},
    "profile_x1":{"type":"integer","description":"upper left crop corner horizontal pixel index (0,0) is at top left corner"},
    "profile_y1":{"type":"integer","description":"upper left crop corner vertical pixel index (0,0) is at top left corner"},
    "profile_x2":{"type":"integer","description":"lower right crop corner horizontal pixel index (0,0) is at top left corner"},
    "profile_y2":{"type":"integer","description":"lower right crop corner vertical pixel index (0,0) is at top left corner"},
    "cover_media_url":{"type":"string","description":""},
    "cover_media_id":{"type":"integer","description":""},
    "cover_x1":{"type":"integer","description":"as above just for cover photo"},
    "cover_y1":{"type":"integer","description":"as above just for cover photo"},
    "cover_x2":{"type":"integer","description":"as above just for cover photo"},
    "cover_y2":{"type":"integer","description":"as above just for cover photo"},
    "facebook_fan_count":{"type":"integer"},
    "facebook_category":{"type":"string"},
    "location_city":{"type":"string"},
    "location_country":{"type":"string"},
    "location_long":{"type":"number","description":"float number of location geo longitude"},
    "location_lat":{"type":"number","description":"float number of location geo latitude"},
    "white_list_audience_names":{"type":"array","items":{"type":"string"}},
    "black_list_audience_names":{"type":"array","items":{"type":"string"}},
    "internal_business_name":{"type":"string"},
    "internal_country":{"type":"string"},
    "internal_email":{"type":"string"},
    "internal_full_address":{"type":"string"},
    "internal_phone_number":{"type":"string"}
  }
}